{
  "name": "reatjs-jwt-auth",
  "version": "0.1.0",
  "description": "React JS authentication system using JWT (JSON Web Tokens) and react-router",
  "keywords": [
    "javascript",
    "react",
    "reactjs",
    "jwt",
    "jwt-authentication",
    "npm",
    "yarn",
    "eslint"
  ],
  "homepage": "https://www.jjpeleato.com/",
  "author": "@jjpeleato",
  "license": "GPL-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjpeleato/reatjs-jwt-auth.git"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "dotenv-cli": "^3.2.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb": "^18.2.0",
    "husky": "^4.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:alpha": "env-cmd -f .env.alpha.local react-scripts build",
    "build:beta": "env-cmd -f .env.beta.local react-scripts build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js src",
    "lint:fix": "eslint --fix --ext .jsx,.js src"
  },
  "husky": {
    "hooks": {
      "post-merge": "yarn install",
      "post-rewrite": "yarn install"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
